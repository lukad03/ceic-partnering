.form-modal
  .close
    span x
  .form-content
    form#form4.wufoo.topLabel.page(name='form4', accept-charset='UTF-8', autocomplete='off', enctype='multipart/form-data', method='post', novalidate='', action='https://piftest.wufoo.com/forms/rlotm2w1j4uqex/#public')
      header#header.info
        h2 Partner with a Lab
        p
          | If you want to partner with a DOE National Laboratory or better
          | understand their capabilities, let us know here what you are
          | interested in exploring.
      ul
        li#foli1.notranslate
          label#title1.desc.hide(for='Field1')
            | Name
          span
            label(for='Field1') First
            input#Field1.field.text.fn(name='Field1', type='text', value='', size='8', tabindex='1', required)
          span
            label(for='Field2') Last
            input#Field2.field.text.ln(name='Field2', type='text', value='', size='14', tabindex='2', required)
        li#foli4.notranslate
          label#title4.desc(for='Field4')
            | Email
          div
            input#Field4.field.text.medium(name='Field4', type='email', spellcheck='false', value='', maxlength='255', tabindex='3', required)
        li#foli3.notranslate
          label#title3.desc(for='Field3')
            | Organization
          div
            input#Field3.field.text.medium(name='Field3', type='text', value='', maxlength='255', tabindex='4', onkeyup='' required)
        li#foli14.notranslate
          label#title14.desc(for='Field14')
            | Select a Lab
          div
            select#Field6.field.select.medium(name='Field14', tabindex='5', onChange='ceic.formTriggers.repeatValue(this, "Field15")' required)
              option(value="" disabled selected)
                  | Select a Lab
              option(value='Ames Laboratory')
                | Ames Laboratory
              option(value='Argonne National Laboratory')
                | Argonne National Laboratory
              option(value='Brookhaven National Laboratory')
                | Brookhaven National Laboratory
              option(value='Fermi National Accelerator Laboratory')
                | Fermi National Accelerator Laboratory
              option(value='Idaho National Laboratory')
                | Idaho National Laboratory
              option(value='Lawrence Berkeley National Laboratory')
                | Lawrence Berkeley National Laboratory
              option(value='Lawrence Livermore National Laboratory')
                | Lawrence Livermore National Laboratory
              option(value='Los Alamos National Laboratory')
                | Los Alamos National Laboratory
              option(value='National Energy Technology Laboratory')
                | National Energy Technology Laboratory
              option(value='National Renewable Energy Laboratory')
                | National Renewable Energy Laboratory
              option(value='Oak Ridge National Laboratory')
                | Oak Ridge National Laboratory
              option(value='Pacific Northwest National Laboratory')
                | Pacific Northwest National Laboratory
              option(value='Princeton Plasma Physics Laboratory')
                | Princeton Plasma Physics Laboratory
              option(value='Sandia National Laboratories')
                | Sandia National Laboratories
              option(value='Savannah River National Laboratory')
                | Savannah River National Laboratory
              option(value='SLAC National Accelerator Laboratory')
                | SLAC National Accelerator Laboratory
          li#foli17.notranslate
            label#title17.desc(for='Field17')
                | Objective
              div
                select#Field17.field.select.medium(name='Field17', tabindex='11' required)
                  option(value="" disabled selected)
                    | Select a Reason for Partnering
                  option(value='Licensing Intellectual Property')
                    | Licensing Intellectual Property
                  option(value='Accessing User Facilities')
                    | Accessing User Facilities
                  option(value='Exploring Partnering Mechanisms')
                    | Exploring Partnering Mechanisms
                  option(value='Learning more about the Lab')
                    | Learn more about a Lab
          label#title7.desc(for='Field7')
            | How Can We Help You?
          div
            textarea#Field7.field.textarea.medium(name='Field7', spellcheck='true', rows='10', cols='50', tabindex='6', onkeyup='', required)
          li#foli9.notranslate.hide
            label#title9.desc(for='Field9')
              | isPending
            div
              input#Field9.field.text.medium(name='Field9', type='text', value='isPending', maxlength='255', tabindex='7', onkeyup='')
          li#foli11.notranslate.hide
            label#title11.desc(for='Field11')
              | tags
            div
              input#Field11.field.text.medium(name='Field11', type='text', value='Partner with Lab', maxlength='255', tabindex='8', onkeyup='')
          li#foli15.notranslate.hide
            label#title15.desc(for='Field15')
              | Source
            div
              input#Field15.field.text.medium(name='Field15', type='text', value='Ames Laboratory', maxlength='255', tabindex='9', onkeyup='')
        li.buttons
          div
            input#saveForm.btTxt.submit(name='saveForm', type='submit', value='Submit')
        li.hide
          label(for='comment') Do Not Fill This Out
          textarea#comment(name='comment', rows='1', cols='1')
          input#idstamp(type='hidden', name='idstamp', value='AnK43sAGhmHot2RtA5EsLz1zjBTTVnvJnbGF30GkeXI=')
